{
  "name": "google-ads-api-report-fetcher",
  "version": "1.3.7",
  "description": "Google Ads API Report Fetcher (gaarf)",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "bin": {
    "gaarf": "gaarf",
    "gaarf-bq": "gaarf-bq"
  },
  "type": "commonjs",
  "engines" : {
    "node" : ">=15.0.0"
  },
  "repository": "google/ads-api-report-fetcher/js",
  "publishConfig":{
    "registry":"https://wombat-dressing-room.appspot.com"
  },
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "tsc": "tsc",
    "start": "node -r ts-node/register src/cli.ts $@",
    "build": "npm run tsc",
    "test": "mocha --ui tdd --require ts-node/register --require src/test/fixtures.ts --timeout 10s src/test/**/*.ts",
    "test:coverage": "nyc --reporter=text mocha --ui tdd --require ts-node/register --require src/test/fixtures.ts --timeout 10s src/test/**/*.ts"
  },
  "keywords": [
    "Google Ads API"
  ],
  "author": "Google Inc. (gTech gPS CSE team)",
  "license": "Apache-2.0",
  "//dependencies": {
    "find-up": "DO NOT upgrade find-up from ^5.0.0"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.12.0",
    "@google-cloud/storage": "^6.0.1",
    "chalk": "^4.1.2",
    "csv": "^6.1.0",
    "csv-stringify": "^6.1.0",
    "find-up": "^5.0.0",
    "google-ads-api": "^10.0.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "table": "^6.8.0",
    "winston": "^3.7.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/glob": "^7.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/yargs": "^17.0.10",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "shx": "^0.3.4",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.4"
  }
}
